<?xml version="1.0"?>

<launch>
  <arg name="refbox" default="atwork_commander"/>
  <arg name="verbose" default="false"/>

  <arg name="robots" value=""/>
    <node name="start_helper" pkg="atwork_commander" type="control" args="-v $(arg verbose) start $(arg robots)" output="screen">
      <param name="refbox" value="$(arg refbox)"/>
    </node>
</launch>
